# -*- python -*-

Import('env')

if not env['HAVE_WX']:
    Return()

files = [
    "BreakpointDlg.cpp",
    "BreakpointView.cpp",
    "BreakpointWindow.cpp",
    "CodeWindow.cpp",
    "CodeWindowFunctions.cpp",
    "MemoryCheckDlg.cpp",
    "MemoryWindow.cpp",
    "RegisterWindow.cpp",
    "RegisterView.cpp",
    "JitWindow.cpp",
    ]

wxenv = env.Clone()
wxenv.Append(
	CPPDEFINES = [
		'wxNEEDS_CHARPP'
		]
	)

libs = [
    'common',
    'debugger_ui_util'
]

wxenv.StaticLibrary(env['local_libs'] + "debwx", files, LIBS = libs)

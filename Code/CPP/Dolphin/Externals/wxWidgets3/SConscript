files = aui + common + generic

if sys.platform == 'win32':
	files += msw
elif sys.platform == 'darwin':
	files += genericosx + osx + unix
else:
	files += genericgtk + gtk + unix + unixgtk

env['LIBS'] += wxenv.StaticLibrary('wx', files)

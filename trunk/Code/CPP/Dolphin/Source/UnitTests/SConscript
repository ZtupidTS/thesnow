# -*- python -*-

Import('env')
import sys

files = [
	"AudioJitTests.cpp",
	"DSPJitTester.cpp",
	"UnitTests.cpp",
	]

libs = [
	'core', 'common',
	]

if sys.platform == 'darwin':
	frames = ['CoreFoundation']
else:
	frames = []

env.Program('tester', files, LIBS = libs, FRAMEWORKS = frames)
